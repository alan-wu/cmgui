			XmNrightOffset = 1;
			XmNbottomAttachment = XmATTACH_POSITION;
			XmNbottomPosition = 2;
			XmNbottomOffset = 1;
			XmNtopAttachment = XmATTACH_POSITION;
			XmNtopPosition = 1;
			XmNtopOffset = 1;
			XmNuserData = gwin_structure;
			};
		callbacks
			{
